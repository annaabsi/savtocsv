    "import pyreadstat\n",
    "import os\n",
    "\n",
    "for year in range(2015, 2021):\n",
    "    for filename in os.listdir('sav/' + str(year)):\n",
    "        df, meta = pyreadstat.read_sav('sav/' + str(year) + '/' + filename)\n",
    "        df.to_csv('csv/' + str(year) + '/' + os.path.splitext(filename)[0] + '.csv')"
   ],
